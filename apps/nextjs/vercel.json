{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd ../.. && bun run build --filter=nextjs",
  "installCommand": "cd ../.. && bun install",
  "framework": "nextjs",
  "github": {
    "silent": true
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 60
    },
    "src/app/api/image-to-prompt/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/image-to-prompt-async/route.ts": {
      "maxDuration": 300
    }
  },
  "regions": ["iad1"],
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  }
}
